import java.util.*;
public class mac18
{
     public static void main(String[] args) {
        int[] arr = {2, 4, 2, 2, 5};
        int highest = arr[0];

        for (int a : arr) {
            if (a > highest) {
                highest = a;
            }
        }

        System.out.println("Highest: " + highest);
    }
}
