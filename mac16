import java.util.*;

public class mac16 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter a number: ");
        int num = sc.nextInt();

        int digits = String.valueOf(Math.abs(num)).length();
        System.out.println("Digits: " + digits);

        sc.close();
    }
}
