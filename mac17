import java.util.*;
public class mac17
{
    public static void main(String[] args) {
        int[] arr = {2, 4, 2, 2, 5};
        int min = arr[0], max = arr[0];

        for (int a : arr) {
            if (a < min) {
                min = a;
            }
            if (a > max) {
                max = a;
            }
        }

        System.out.println("Smallest: " + min);
        System.out.println("Largest: " + max);
    }
}
