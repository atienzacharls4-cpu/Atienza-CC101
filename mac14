import java.util.*;
public class mac14
{
     public static void main(String[] args) {
        int[] arr = {2, 4, 2, 2, 5};
        int target = 2;
        int count = 0;

        for (int a : arr) {
            if (a == target) {
                count++;
            }
        }

        System.out.println("Appears at least 3 times? " + (count >= 3));
    }
}
