import java.util.*;
public class mac19
{
     public static void main(String[] args) {
        int[] arr = {2, 4, 2, 2, 5};
        int h1 = Integer.MIN_VALUE;
        int h2 = Integer.MIN_VALUE;

        for (int a : arr) {
            if (a > h1) {
                h2 = h1;
                h1 = a;
            } else if (a > h2 && a != h1) {
                h2 = a;
            }
        }

        System.out.println("2nd Highest: " + h2);
    }
}
