public class mac11 {
    public static void main(String[] args) {
        String[] arr = {"A", "B", "C", "D", "E", "F"};

        System.out.print("Even-index elements: ");
        for (int i = 0; i < arr.length; i += 2) { // jumps by 2 to get 0,2,4...
            System.out.print(arr[i] + " ");
        }
    }
}
