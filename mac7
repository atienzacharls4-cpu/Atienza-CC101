import javax.swing.*;
public class mac7
{
    public static void main (String [] args){
        int[] arr = {5, 6, -8, 9, -22};

        for (int i = 0; i < arr.length; i++) {
            arr[i] = Math.abs (arr[i]);
        }

        for (int num : arr) {
            JOptionPane.showMessageDialog(null, num + " ");
        }
    
    }
}
