import java.util.*;

public class mac13 {
    public static void main(String[] args) {
        int[] grades = {98, 67, 74, 81, 85, 72};
        int passed = 0, failed = 0;

        for (int g : grades) {
            if (g >= 75) passed++; // passing condition
            else failed++; // otherwise it's failed
        }

        System.out.println("Passed: " + passed);
        System.out.println("Failed: " + failed);
    }
}

