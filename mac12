import java.util.*;
public class mac12
{
     public static void main(String[] args) {
        int[] numbers = {5, -3, -8, 10};

        for (int i = 0; i < numbers.length; i++) {
            if (numbers[i] < 0) {
                numbers[i] = -numbers[i];
            }
        }

        System.out.print("Positive converted: ");
        for (int n : numbers) {
            System.out.print(n + " ");
        }
    }
}
