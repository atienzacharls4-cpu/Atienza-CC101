public class mac12 {
    public static void main(String[] args) {
        int[] arr = {5, 2, 5, 5, 1, 5};
        int target = 5;
        int count = 0;

        for (int n : arr) { 
            if (n == target) count++;
        }

        System.out.println("Appears at least 3 times? " + (count >= 3));
    }
}
